l-dropdown>\n      </el-header>\n      <!-- 页面主题区域 -->\n      <el-container background-color=\"#2b4b6b\">\n        <!-- 卡片视图区域-->\n        <el-card class=\"el-index-aside\">\n          <el-card class=\"el-index-aside-1\">\n            <div class=\"admin-dev\">\n              <span class=\"admin\">API测试平台</span>\n            </div>\n            <div>\n              <span class=\"introduction\">简介：测试部API测试工具平台</span>\n            </div>\n          </el-card>\n          <div>\n            <el-menu text-color=\"#363636\"\n                     active-text-color=\"#409EFF\"\n                     :unique-opened=\"true\"\n                     :collapse-transition=\"false\"\n                     :default-active=\"activeName\">\n              <el-menu-item v-for=\"item in projectListResList\"\n                            :key=\"item.project_id\"\n                            :index=\"item.project_id\"\n                            @click=\"saveNavState(item.project_id),modelList(item.project_id)\">\n                <i class=\"el-icon-folder-opened\"></i>\n                <span slot=\"title\">{{item.project_name}}</span>\n              </el-menu-item>\n            </el-menu>\n          </div>\n          <el-pagination layout=\"prev, pager, next\"\n                         :current-page=\"projectListBody.page_num\"\n                         @current-change=\"handleCurrentChange\"\n                         :total=\"50\">\n          </el-pagination>\n        </el-card>\n        <el-card class=\"el-index-aside-two\">\n\n        </el-card>\n      </el-container>\n    </el-container>\n  </div>\n</template>\n\n<script>\nexport default {\n  data () {\n    return {\n      loginname: {\n        name: ''\n      },\n      updatePasword: false,\n      projectListBody: {\n        page_num: 1\n      },\n      modelListBody: {\n        project_id: ''\n      },\n      projectListResList: {},\n      modelListResList: {}\n    }\n  },\n  created () {\n    this.activePath = window.sessionStorage.getItem('activePath')\n    this.loginname.name = window.sessionStorage.getItem('loginName')\n    this.projectList()\n  },\n  methods: {\n    saveNavState (activeName) {\n      window.sessionStorage.setItem('activeName', activeName)\n      this.activeName = activeName\n    },\n    logout () {\n      window.sessionStorage.clear()\n      this.$router.push('/login')\n    },\n    // 获取所有项目列表\n    async projectList () {\n      const { data: projectRes } = await this.$api.project.getProjectList(\n        this.projectListBody\n      )\n      if (projectRes.code !== 1) {\n        return this.$message.error('获取项目列表失败！')\n      }\n      this.projectListResList = projectRes.data\n    },\n    // 获取所有模块列表\n    async modelList (id) {\n      this.modelListBody.project_id = id\n      const { data: responseBody } = await this.$api.project.getModelList(\n        this.modelListBody\n      )\n      if (responseBody.code !== 1) {\n        return this.$message.error('获取用户列表失败！')\n      }\n      this.modelListResList = responseBody.data\n    },\n    // 监听 页码值改变的事件\n    handleCurrentChange (newPage) {\n      this.projectListBody.page_num = newPage\n      this.projectList()\n    }\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.el-index-menu {\n  padding: 0px;\n}\n.index_container {\n  background-color: #dddddd;\n  height: 100%;\n}\n.el-index-aside {\n  width: 20%;\n  margin-top: 25px;\n  margin-bottom: 25px;\n  margin-left: 25px;\n  margin-right: 10px;\n}\n.el-index-aside-1 {\n  background-color: #373d41;\n  height: 130px;\n  .admin-dev {\n    margin-bottom: 20px;\n    margin-left: 5px;\n  }\n  .admin {\n    color: hsla(0, 0%, 100%, 0.85);\n    font-size: 30px;\n  }\n  .introduction {\n    color: hsla(0, 0%, 100%, 0.85);\n    font-size: 12px;\n  }\n}\n.el-index-aside-two {\n  width: 78%;\n  margin-top: 25px;\n  margin-bottom: 25px;\n  margin-left: 0px;\n  margin-right: 25px;\n}\n.home-container {\n  height: 100%;\n}\n.el-dropdown-link {\n  font-size: 20px;\n  cursor: pointer;\n  color: #fff;\n}\n.el-header {\n  background-color: #373d41;\n  display: flex;\n  justify-content: space-between;\n  padding-left: 0%;\n  align-items: center;\n  color: #ffffff;\n  font-size: 20px;\n  > div {\n    display: flex;\n    align-items: center;\n    span {\n      margin-left: 15px;\n    }\n    img {\n      width: 120px;\n      height: 100%;\n      background-color: #373d41;\n      margin-left: 40px;\n      margin-right: 40px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}